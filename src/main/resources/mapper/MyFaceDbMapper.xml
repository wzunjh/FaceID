<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.face.mapper.MyFaceDbMapper">

    <resultMap id="BaseResultMap" type="com.face.bean.MyFaceDb">
            <id property="fid" column="fid" jdbcType="INTEGER"/>
            <result property="faceBase" column="face_base" jdbcType="VARCHAR"/>
            <result property="faceName" column="face_name" jdbcType="VARCHAR"/>
            <result property="faceId" column="face_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        fid,face_base,face_name,face_id
    </sql>
</mapper>
